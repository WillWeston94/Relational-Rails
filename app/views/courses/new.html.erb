<h1>Register Course</h1>
<p><a href="/courses">Our Courses</a></li></p>
<form action="<%= courses_path(@faculty) %>" method="post">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  
  <p>Enter course details:</p>

  <div>
    <label for="course_name">Course Name:</label>
    <input type="text" name="[course_name]" id="course_name">
  </div>

  <div>
    <label for="course_professor">Course Professor:</label>
    <input type="text" name="[professor]" id="professor">
  </div>

  <div>
    <label for="course_gen_ed">Fulfills General Education:</label>
    <input type="checkbox" name="[gen_ed]" id="gen_ed">
  </div>

  <div>
    <label for="course_credits">Credits:</label>
    <input type="number" name="[credits]" id="credits">
  </div>

  <div>
    <label for="course_start_date">Start Date:</label>
    <input type="text" name="[start_date]" id="start_date">
  </div>

  <div>
    <label for="course_end_date">End Date:</label>
    <input type="text" name="[end_date]" id="end_date">
  </div>

  <input type='submit' value='Register Course'/>
</form>